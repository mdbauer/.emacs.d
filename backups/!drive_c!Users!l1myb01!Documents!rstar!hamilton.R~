df <- read.csv("data/employment.csv", header=FALSE) # from Hamilton replication file
names(df) <- c("Date", "nfp_sa", "nfp_nsa")

y <- ts(100*log(df$nfp_sa), start=min(df$Date), freq=4)
require(dynlm)

h <- 8

mod <- dynlm(y ~ L(y, h) + L(y, h+1) + L(y, h+2) + L(y, h+3))
print(summary(mod))
c <- resid(mod)
cat("SD =", sd(c), "\n")
crw <- diff(y, h)
cat("SD (diff) =", sd(crw), "\n")


