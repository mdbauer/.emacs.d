\begin{figure}
  \caption{Ten-year yield and macroeconomic trends}
  \label{fig:data1}\centering
  \vspace{.5pc}
  \includegraphics{../../figures/data1.pdf}
  \begin{flushleft}
    \small Ten-year Treasury yield and estimates of trend inflation,
    $\pi^\ast$ (the mostly survey-based PTR measure from FRB/US), and of the equilibrium real rate, $r^\ast$ (the average of the estimates from \cite{laubach-williams}, \cite{lubik-mathes}, \cite{kiley} and \cite{johannsen-mertens}).
  \end{flushleft}
\end{figure}

